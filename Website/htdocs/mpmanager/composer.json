{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "doctrine/dbal": "2.10.1",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "7.2.0",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.2",
        "laravel/horizon": "^4.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0.3",
        "mpociot/laravel-apidoc-generator": "^4.4",
        "phpmailer/phpmailer": "^6.1.5",
        "phpoffice/phpspreadsheet": "1.11.0",
        "predis/predis": "1.1.1",
        "pusher/pusher-php-server": "4.1.1",
        "tymon/jwt-auth": "1.0.0",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "facade/ignition": "^2.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "*",
        "vimeo/psalm": "^4.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Inensus\\Ticket\\": "packages/inensus/ticket/src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": "find . -name \\*.php -not -path './lib/composer/*' -not -path './build/.phan/*' -exec php -l \"{}\" \\;"
    }

}
